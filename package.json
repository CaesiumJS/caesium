{
  "name": "caesium",
  "version": "0.0.1-alpha7",
  "description": "React based static site generator",
  "main": "index.js",
  "bin": {
    "caesium": "./bin/caesium.js"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha ./tests/*.js",
    "jscover": "./node_modules/.bin/jscoverage ./lib ./lib-cov",
    "coverage": "env COVERAGE=1 ./node_modules/.bin/mocha ./tests/*.js -R mocha-lcov-reporter > results.lcov"
  },
  "repository": "https://github.com/CaesiumJS/caesium.git",
  "keywords": [
    "react",
    "static",
    "site",
    "generator",
    "caesium"
  ],
  "author": "Adam Laycock <adam@arcath.net>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.3.5",
    "browserify": "^13.0.0",
    "chokidar": "^1.4.3",
    "commander": "^2.9.0",
    "defaults": "^1.0.3",
    "front-matter": "^2.0.7",
    "mime": "^1.3.4",
    "mkdirp": "^0.5.1",
    "progress": "^1.1.8",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "remarkable": "^1.6.2",
    "rimraf": "^2.5.2",
    "semver": "^5.1.0",
    "sodb": "^1.1.4",
    "static-server": "^2.0.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "jscoverage": "^0.6.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
